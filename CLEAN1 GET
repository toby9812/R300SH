INH
LETVI V19%,80
MOVEX A=1, AC=0, SM=0, M1X, P, P40, S=5000, H=1, MS	'P40
INH
LETVI V19%,40
MOVEX A=1, AC=1, SM=0, M1X, P, P60, S=4800, H=1, MS	'P60
INH
LETVI V19%,60
GOTO *START
ENDIF
'  
*START
MOVEX A=1, AC=1, SM=0, M1X, P, P60, R=V31%, H=1, MS	'P60
INH
LETVI V19%,60
